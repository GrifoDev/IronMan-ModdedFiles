        <level id="1" temp="430" />
        <level id="2" temp="450" />
        <level id="3" temp="470" />
        <level id="4" temp="490" />
        <level id="5" temp="510" />
        <level id="6" temp="530" />
    </levels>

    <default_scenario>
        <limiter name="CPUFreqMax" type="number">
            <table_item temp="430" value="-1" />
            <table_item temp="450" value="1248000" />
            <table_item temp="470" value="1094400" />
            <table_item temp="490" value="960000" />
        </limiter>
        <limiter name="GPUFreqMax" type="number">
            <table_item temp="430" value="-1" />
